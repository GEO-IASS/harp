  <cd:ProductType name="ML2BRO">
    <cd:Description>L2 BrO</cd:Description>
    <cd:ProductDefinition id="ML2BRO" format="hdf5" version="0">
      <cd:Description>The format definition for this product is taken from the HDF5 file itself.</cd:Description>
      <cd:DetectionRule>
        <cd:MatchExpression path="/HDFEOS/ADDITIONAL/FILE_ATTRIBUTES">str(.@InstrumentName,3) == "MLS"</cd:MatchExpression>
        <cd:MatchExpression>str(.@ProcessLevel,1) == "2" || str(.@ProcessLevel,2) == "L2"</cd:MatchExpression>
        <cd:MatchExpression path="/HDFEOS/SWATHS">exists(./BrO)</cd:MatchExpression>
      </cd:DetectionRule>
    </cd:ProductDefinition>
  </cd:ProductType>
